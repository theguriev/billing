{
  "/authorization/change-name": {
    "methods": ["PUT"],
    "authorizationNeeded": true,
    "proxy": {
      "to": "http://authorization:3000/change-name"
    }
  },
  "/authorization": {
    "methods": ["GET"],
    "authorizationNeeded": true,
    "proxy": {
      "to": "http://authorization:3000"
    }
  },
  "/authorization/login": {
    "methods": ["POST"],
    "proxy": {
      "to": "http://authorization:3000/login"
    }
  },
  "/authorization/registration": {
    "methods": ["POST"],
    "proxy": {
      "to": "http://authorization:3000/registration"
    }
  },
  "/authorization/logout": {
    "methods": ["GET"],
    "proxy": {
      "to": "http://authorization:3000/logout"
    }
  },
  "/authorization/refresh": {
    "methods": ["GET"],
    "proxy": {
      "to": "http://authorization:3000/refresh"
    }
  },
  "/billing/signature": {
    "methods": ["POST"],
    "proxy": {
      "to": "http://billing:3000/signature"
    }
  },
  "/billing/token": {
    "methods": ["GET", "POST"],
    "proxy": {
      "to": "http://billing:3000/token"
    }
  },
  "/billing/transaction/:symbol": {
    "methods": ["GET", "POST"],
    "proxy": {
      "to": "http://billing:3000/transaction/:symbol"
    }
  },
  "/billing/wallet/:key": {
    "methods": ["GET"],
    "proxy": {
      "to": "http://billing:3000/wallet/:key"
    }
  },
  "/billing/wallet": {
    "methods": ["GET"],
    "proxy": {
      "to": "http://billing:3000/wallet"
    }
  }
}
