{
  "/billing/token": {
    "methods": ["GET", "POST"],
    "proxy": {
      "to": "http://billing:3000/token"
    }
  },
  "/billing/token/issue": {
    "methods": ["POST"],
    "proxy": {
      "to": "http://billing:3000/token/issue"
    }
  },
  "/billing/transaction/:symbol": {
    "methods": ["GET", "POST"],
    "proxy": {
      "to": "http://billing:3000/transaction/:symbol"
    }
  },
  "/billing/wallet/:key": {
    "methods": ["GET"],
    "proxy": {
      "to": "http://billing:3000/wallet/:key"
    }
  },
  "/billing/wallet": {
    "methods": ["GET"],
    "proxy": {
      "to": "http://billing:3000/wallet"
    }
  }
}
