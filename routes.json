{
  "/billing/ballance/:address": {
    "methods": ["GET"],
    "proxy": {
      "to": "http://billing:3000/ballance/:address"
    }
  },
  "/billing/tokens": {
    "methods": ["POST", "GET"],
    "proxy": {
      "to": "http://billing:3000/tokens"
    }
  },
  "/billing/tokens/:id": {
    "methods": ["GET"],
    "proxy": {
      "to": "http://billing:3000/tokens/:id"
    }
  },
  "/billing/tokens/issue": {
    "methods": ["POST"],
    "proxy": {
      "to": "http://billing:3000/tokens/issue"
    }
  },
  "/billing/transactions": {
    "methods": ["GET", "POST"],
    "proxy": {
      "to": "http://billing:3000/transactions"
    }
  },
  "/billing/transactions/stats": {
    "methods": ["GET"],
    "proxy": {
      "to": "http://billing:3000/transactions/stats"
    }
  },
  "/billing/transactions/summary": {
    "methods": ["GET"],
    "proxy": {
      "to": "http://billing:3000/transactions/summary"
    }
  }
}
